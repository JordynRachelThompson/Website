@model IEnumerable<MyWebsite.Models.BudgetProject.BudgetItems>
@{
    ViewData["Title"] = "PastBudgets";
}

@{
    string user = User.Identity.Name;
}

@*Does user have a budget for a particular month*@

<div style="text-align:center; margin-top:80px;"><p style="text-align:center;">@Html.Partial("_LoginPartial")</p></div>
<h3 style="text-align:center; color:#cfd8dc; font-weight:bold; margin-top:100px;">The Painless Budget</h3>
<div style="border-bottom: 1px dotted #cfd8dc;"></div>

<div class="container">
    <div style="margin-top:55px; text-align:center;">
        <p style="text-align:center; font-weight:bold; color:#76ff03; margin-top:10px;">@ViewBag.Deleted</p>
        <a href='@Url.Action("Index", "Budgets", new { userName = user })' ; class="waves-effect waves-light btn-large sharpCorners grey darken-4" style="font-weight:bold; font-size:20px;"><i class="material-icons left">arrow_back</i>Return to Budget</a>
    </div>
    <h5 style="margin-top:55px; margin-bottom:25px; font-weight:bold; color:#b2dfdb; text-align:center">Past Budgets</h5>

    @*Display month buttons only for months with budgets*@
    <div style="text-align:center">
        @if (Convert.ToBoolean(TempData["jan"]))
        {
            <button id="jan" class="waves-effect waves-light btn-large sharpCorners grey darken-4" style="font-weight:bold; font-size:20px; margin-top:10px;" onclick="location.href='@Url.Action("ItemizedTransaction", "Budgets", new { month = 1 } )'">January</button>

        }
        @if (Convert.ToBoolean(TempData["feb"]))
        {
            <button id="feb" class="waves-effect waves-light btn-large sharpCorners grey darken-4" style="font-weight:bold; font-size:20px; margin-top:10px;">February</button>

        }
        @if (Convert.ToBoolean(TempData["march"]))
        {
            <button id="march" class="waves-effect waves-light btn-large sharpCorners grey darken-4" style="font-weight:bold; font-size:20px; margin-top:10px;">March</button>

        }
        @if (Convert.ToBoolean(TempData["apr"]))
        {
            <button id="apr" class="waves-effect waves-light btn-large sharpCorners grey darken-4" style="font-weight:bold; font-size:20px; margin-top:10px;">April</button>

        }
        @if (Convert.ToBoolean(TempData["may"]))
        {
            <button id="may" class="waves-effect waves-light btn-large sharpCorners grey darken-4" style="font-weight:bold; font-size:20px; margin-top:10px;">May</button>

        }
        @if (Convert.ToBoolean(TempData["june"]))
        {
            <button id="june" class="waves-effect waves-light btn-large sharpCorners grey darken-4" style="font-weight:bold; font-size:20px; margin-top:10px;">June</button>

        }
        @if (Convert.ToBoolean(TempData["july"]))
        {
            <button id="july" class="waves-effect waves-light btn-large sharpCorners grey darken-4" style="font-weight:bold; font-size:20px; margin-top:10px;">July</button>

        }
        @if (Convert.ToBoolean(TempData["aug"]))
        {
            <button id="aug" class="waves-effect waves-light btn-large sharpCorners grey darken-4" style="font-weight:bold; font-size:20px; margin-top:10px;">August</button>

        }
        @if (Convert.ToBoolean(TempData["sept"]))
        {
            <button id="sept" class="waves-effect waves-light btn-large sharpCorners grey darken-4" style="font-weight:bold; font-size:20px; margin-top:10px;">September</button>

        }
        @if (Convert.ToBoolean(TempData["oct"]))
        {
            <button id="oct" class="waves-effect waves-light btn-large sharpCorners grey darken-4" style="font-weight:bold; font-size:20px; margin-top:10px;">October</button>

