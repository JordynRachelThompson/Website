@*ONE - JAN*@
<div class="container center" id="itemizedBudgetContainer1" style="margin-top:35px; margin-bottom:50px; display:none;">
    <h5 style="margin-top:55px; margin-bottom:35px; font-weight:bold; color:#cfd8dc; text-align:center">January Transactions</h5>
    <a class="waves-effect waves-light lightBrown-hover btn-large sharpCorners grey darken-4" id="transaction-btnG1" style="font-weight:bold; font-size:20px; margin-top:10px;"><i class="material-icons left">local_dining</i>Grocery</a>
    <table class="table" id="transactionsG1" align="center" style="display:none; width:25%; margin-bottom:25px;">
        <tr style="border-bottom: 1px dotted #cfd8dc">
            <th>Date</th>
            <th>Description</th>
            <th>Price</th>
            <th></th>
        </tr>
        @{ int count1 = 0;}
        @foreach (var item in Model.Where(x => x.Month == 1).Where(x => Convert.ToInt32(x.TypeOfBudget) == 1).OrderBy(x => x.Date))
        {
            count1 += 1;
            <tr style="border-bottom: 1px dotted #cfd8dc">
                <td>@item.Date.ToString("MM/dd/yyyy")</td>
                <td>@item.Description</td>
                <td>$@item.Cost</td>
                <td><a href='@Url.Action("DeletePastTransaction", "Budgets", new { id = item.TransactionId })'><i class="material-icons left trashcan-icon">delete</i></a></td>
            </tr>
        }

        @if (count1 == 0)
        {
            <tr>
                <th>No Transactions...</th>
            </tr>
        }
    </table>
    <a class="waves-effect waves-light btn-large lightBrown-hover sharpCorners grey darken-4" id="transaction-btnH1" style="font-weight:bold; font-size:20px; margin-top:10px;"><i class="material-icons left">home</i>Housing</a>
